/*Подсчитать общее количество лайков, которые получили 10 самых молодых пользователей.*/
SELECT 
    SUM(likes)
FROM
    (SELECT 
        profiles.users_id,
            profiles.birthday,
            (SELECT 
                    COUNT(*)
                FROM
                    VK_class.likes
                WHERE
                    likes.users_id = profiles.users_id) AS likes
    FROM
        VK_class.profiles
    GROUP BY profiles.users_id , profiles.birthday
    ORDER BY profiles.birthday DESC
    LIMIT 10) t;
